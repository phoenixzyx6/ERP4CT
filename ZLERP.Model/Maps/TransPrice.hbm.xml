<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.TransPrice, ZLERP.Model" table="TransPrice" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="TransPriceID">
		 	<generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="PriceDate" column="PriceDate" />
		<property name="UnitPrice" column="UnitPrice" />
		<property name="CalcType" column="CalcType" />
		<property name="Remark" column="Remark" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" />
    <property name="TransportID"/>
    <property name="SupplyID"/>
		<many-to-one name="TransportInfo" column="TransportID" class="SupplyInfo" update="false" insert="false"/>
		<many-to-one name="SupplyInfo" column="SupplyID" class="SupplyInfo" update="false" insert="false"/>
	</class>
</hibernate-mapping>