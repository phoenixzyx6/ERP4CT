<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.StuffAccountMain, ZLERP.Model" table="StuffAccountMain" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="StuffAccountMainID">
			<generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="Month" column="Month" />
		<property name="StartDate" column="StartDate" />
		<property name="EndDate" column="EndDate" />
		<property name="Meno" column="Meno" />
		<property name="BuildTime" column="BuildTime" update="false"/>
		<property name="Builder" column="Builder" update="false"/>
		<property name="ModifyTime" column="ModifyTime" insert="false"/>
		<property name="Modifier" column="Modifier" insert="false"/>
		<property name="Lifecycle" column="Lifecycle" />

		<bag name="StuffAccounts" lazy="true" cascade="all" inverse="true" >
			<key column="StuffAccountMainId"></key>
			<one-to-many class="StuffAccount"></one-to-many>
		</bag>
	</class>
</hibernate-mapping>