<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.TyreInfo, ZLERP.Model" table="TyreInfo" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="TryeID">
		 	<generator class="assigned" />
		</id>
		<version name="Version" column="Version" />
		<property name="StockDate" column="StockDate" />
		<property name="TyreType" column="TyreType" />
		<property name="TyreModel" column="TyreModel" />
		<property name="TyreSpec" column="TyreSpec" />
		<property name="InstallPlace" column="InstallPlace" />
		<property name="Manufacture" column="Manufacture" />
		<property name="BreedCode" column="BreedCode" />
		<property name="MileageAble" column="MileageAble" />
		<property name="MileageUsed" column="MileageUsed" />
		<property name="CurrentStatus" column="CurrentStatus" />
		<property name="CarID" column="CarID" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" /> 
	</class>
</hibernate-mapping>