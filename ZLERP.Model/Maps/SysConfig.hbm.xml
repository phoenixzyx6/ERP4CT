<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2" namespace="ZLERP.Model" assembly="ZLERP.Model">
	<class name="ZLERP.Model.SysConfig, ZLERP.Model" table="SysConfig" lazy="true"
    dynamic-insert="true" dynamic-update="true" batch-size="30" optimistic-lock="version">
		<id name="ID" column="SysConfigID">
		 	<generator class="native" />
		</id>
		<version name="Version" column="Version" />
		<property name="ConfigName" column="ConfigName" />
		<property name="DateType" column="DateType" />
		<property name="ConfigValue" column="ConfigValue" />
		<property name="ConfigInfo" column="ConfigInfo" />
    <property name="Remark" column="Remark" />    
    <property name="ConfigType" column="ConfigType" />
		<property name="OrderNum" column="OrderNum" />
    <property name="BuildTime" column="BuildTime" update="false"/>
    <property name="Builder" column="Builder" update="false"/>
    <property name="ModifyTime" column="ModifyTime" insert="false"/>
    <property name="Modifier" column="Modifier" insert="false"/>
    <property name="Lifecycle" column="Lifecycle" /> 
	</class>
</hibernate-mapping>