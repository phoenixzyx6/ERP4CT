                <table class="mvcform">
                    <tr>
                        <td>@Html.LabelFor(m => m.IsOut)@Html.CheckBoxFor(m => m.IsOut)
                        </td>
                        <td>@Html.LabelFor(m => m.Slump)@Html.EditorFor(m => m.Slump)
                        </td>
                        <td>@Html.LabelFor(m => m.MixGrade)@Html.EditorFor(m => m.MixGrade)
                        </td>
                        <td>@Html.LabelFor(m => m.OtherCap)@Html.EditorFor(m => m.OtherCap)
                        </td>
                    </tr>
                    <tr>
                        <td>@Html.LabelFor(m => m.CheckResult)@Html.EditorFor(m => m.CheckResult)
