@model ZLERP.Model.OpenCheck 
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/OpenCheck/OpenCheck.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            opencheckIndexInit('@Url.Action("Find", "OpenCheck")',
            '@Url.Action("Get", "ProduceTask")',
            '@Url.Action("Get", "CustMixprop")',
            '@Url.Action("GetItemsAmountById", "CustMixprop")',
            '@Url.Action("Get", "OpenCheck")',
            '@User.UserName()'
            );
        });
    </script>
}
<div id="MyJqGrid">
</div>
