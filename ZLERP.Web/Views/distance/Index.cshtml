@model ZLERP.Model.distance
@section HeaderSection{
<script src="@Url.Content("~/Scripts/Views/distance.js")" type="text/javascript" language="javascript"></script>
<script type="text/javascript">
    $(function () {
        projectIndexInit({
            storeUrl: '@Url.Action("Find")',
            projectUpdateUrl: '@Url.Action("Update")'
        });
    });
</script>
}
<div id="projectForm" style="display:none;">
@using (Html.BeginForm())
{
@Html.ValidationSummary()
<table class="mvcform">
    <tr> 
        @Html.TextBoxFor(m => m.ID, new { @class = "text", style = "display:none" })        
        @*<td>@Html.LabelFor(m => m.ProjectName)@Html.DropDownListFor(m => m.projectid, (IEnumerable<SelectListItem>)ViewBag.Project, new { style = "width:137px;" })</td>*@

        <td>@Html.LabelFor(m => m.projectid)@Html.AutoComplete("projectid", "ProjectName", "Combo", "Project", "ID", "ProjectName", " isShow = 1 ", new { style = "width:140px" })</td>
        @*<td>@Html.LabelFor(m => m.CastModeid)@Html.DicEditorFor(m => m.CastModeid, DicEnum.CastMode, new { style = "width:140px;" })</td>*@
        <td>@Html.LabelFor(m => m.CastModeid)@Html.AutoComplete("CastModeid", "DicName", "Combo", "Dic", "ID", "DicName", " ParentID='CastMode' ", new { style = "width:140px" })</td>
    </tr>
    <tr>
        <td>@Html.LabelFor(m => m.distance)@Html.TextBoxFor(m => m.distance)</td>
    </tr>
    
</table>
}
</div>
<div id="ProjectGrid"></div>
