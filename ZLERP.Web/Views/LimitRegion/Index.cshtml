@model ZLERP.Model.LimitRegion
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/LimitRegion/LimitRegion.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            limitRegionIndexInit({
                LimitRegionStoreUrl: '@Url.Action("Find")',
                LimitRegionDeleteUrl : '@Url.Action("Delete")'
             });
        });
    </script>
}
<div id="LimitRegionForm" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
<table class="mvcform">
    <tr> 
        <td>@Html.TextBoxFor(m=>m.ID, new { @class = "text",style="display:none" })</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.Name)@Html.TextBoxFor(m => m.Name, new { @class = "text" })</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.IsOutAlarm)@Html.CheckBoxFor(m=>m.IsOutAlarm)</td>
        <td>@Html.LabelFor(m=>m.IsShow)@Html.CheckBoxFor(m => m.IsShow)</td>
   
    </tr>
     
</table>
}
</div>
<div id="LimitRegionGrid"></div> 