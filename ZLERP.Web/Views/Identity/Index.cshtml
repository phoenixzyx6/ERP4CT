@model ZLERP.Model.Identity
@section HeaderSection{
    <script src="@Url.Content("~/Scripts/Views/Identity.js")" type="text/javascript" language="javascript"> </script>
    <script type="text/javascript">
        $(function () {
            identityIndexInit('@Url.Action("Find")');
        });
    </script>
}
<div id="IdentityForm" style="display:none;">
@using (Html.BeginForm())
{@Html.ValidationSummary()
<table class="mvcform">
    <tr> 
        <td>@Html.TextBoxFor(m=>m.ID, new { @class = "text",style="display:none" })</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.IdentityCode)@Html.TextBoxFor(m=>m.IdentityCode, new { @class = "text" })</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.IdentityType)@Html.DicEditorFor(m=>m.IdentityType,  DicEnum.IdenType )</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.IdentityName)@Html.TextBoxFor(m=>m.IdentityName, new { @class = "text" })</td>
   
    </tr>
    <tr> 
        <td>@Html.LabelFor(m=>m.IdentityPrice)@Html.TextBoxFor(m=>m.IdentityPrice, new { @class = "text" })</td>
    </tr>
</table>
}
</div>
<div id="IdentityGrid"></div> 