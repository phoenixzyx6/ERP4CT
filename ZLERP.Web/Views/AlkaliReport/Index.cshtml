                </td>
                <td>@Html.LabelFor(m => m.ADM3ExamID)@Html.EditorFor(m => m.ADM3ExamID)
                </td>
                <td>@Html.LabelFor(m => m.ADM3SupplyID)@Html.EditorFor(m => m.ADM3SupplyID)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.ADM3Amount)@Html.EditorFor(m => m.ADM3Amount)
