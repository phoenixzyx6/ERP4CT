                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.AIR2Amount)@Html.EditorFor(m => m.AIR2Amount)
                </td>
                <td>@Html.LabelFor(m => m.AIR2AlkPer)@Html.EditorFor(m => m.AIR2AlkPer)
                </td>
