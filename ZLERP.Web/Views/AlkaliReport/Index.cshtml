                <td>@Html.Label("碱含量KG")@Html.TextBox("AIR1AlkAmount", 0)
                </td>
            </tr>
            <tr>
                <td>@Html.LabelFor(m => m.AIR2StuffID)@Html.DropDownListFor(m => m.AIR2StuffID, (IEnumerable<SelectListItem>)ViewBag.AIRStuff)
